{
  "expo": {
    "name": "vbox",
    "slug": "vbox",
    "version": "1.0.0",
    "scheme": "vbox",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/vbox.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true,
          "supportsPictureInPicture": true
        }
      ],
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/vbox.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/vbox.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/vbox.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.chinedumeke.vbox"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "164ea9ad-7da8-4359-89e2-9593ce1a196c"
      }
    }
  }
}
